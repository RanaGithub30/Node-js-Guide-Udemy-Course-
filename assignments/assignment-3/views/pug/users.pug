extends ./layouts/header
block cssContent
    link(rel="stylesheet", href="/css/main.css")
block content
    if userNames.length > 0
    each u_name in userNames
        .card 
            img(src="data:image/png;base64," alt="Avatar")
            .container 
                h4 
                    b #{u_name.name} 
                p Description 
                button(type="button") Details
    else 
        p No User Found